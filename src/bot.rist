// from discord.ext import commands;
+@ discord.ext @+ commands;

// Define bot as an instance of commands.Bot;
bot = commands.Bot{command_prefix="!"} // Prefix is !;

// !hello command;
@bot.command{};
async define hello{ctx}:;
  await ctx.reply{"hello"} // response of command;

// Load an extension (compiled by --compile-to src/cog/say);
bot.load_extension{"src.cogs.say"};
// Run it with token;
bot.run{"token"};
